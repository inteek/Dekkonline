                <div class="tab-pane" id="tab-pending">
                    @Html.Action("Pending", "Profile")

                </div>
                <!-- tab PAST -->
                <div class="tab-pane" id="tab-past">
                    @Html.Action("Past", "Profile")

                </div>
                <!-- tab PROMOS -->
                <div class="tab-pane" id="tab-promos">
                    @Html.Action("Promos", "Profile")
                </div>
            </div>
        </div>
        <!-- tab style -->
    </div>

    @Scripts.Render("~/scripts/app.js")
